class Swapp{
public static void main(String args[])
{
int a=8 ,b=6,temp;
System.out.println("Before swap a="+a+"and b="+b);

temp=a;
a=b;
b=temp;
System.out.println("After swap a="+a+"and b="+b);
System.out.println("Again Swap");
a=a+b;
b=a-b;
a=a-b;
System.out.println("After again swap a="+a+"and b="+b);

}
}
